{
    "require": [
        "webserver_php_magento-module-demo.json",
        "database_magento2-module-demo.json",
        "elasticsearch_magento-module-demo.json",
        "mailcatcher_magento.json",
        "magento2-module-demo.json"
    ],
    "global": {
        "systemName": "infrangible_m2_etrusted_hyva"
    },
    "ws": {
        "webServerType": "apache_php",
        "webServerVersion": "2.4",
        "phpVersion": "8.2",
        "containerVolumes": [
            "../src:/var/www/magento/htdocs/app/code/Infrangible/ETrustedHyva:www-data"
        ],
        "beforeContainerInstallDockerScript": "add-hyva-theme.sh",
        "beforeContainerInstallDockerUser": "<webServerUser>"
    },
    "db": {
        "databaseType": "mysql",
        "databaseVersion": "5.7"
    },
    "es": {
        "elasticsearchVersion": "7.9"
    },
    "mc": {
        "mailCatcherVersion": "0.6"
    },
    "ms": {
        "magentoVersion": "2.4.6-p3",
        "magentoEdition": "community"
    }
}
